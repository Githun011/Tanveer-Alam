procedure fibonacci : fib_num
   
   IF fib_num less than 1
      DISPLAY 0
      
   IF fib_num equals to 1
      DISPLAY 1
      
   IF fib_num equals to 2
      DISPLAY 1, 1
      
   IF fib_num greater than 2
      Pre = 1,
      Post = 1,
      
      DISPLAY Pre, Post
      FOR 0 to fib_num-2
         Fib = Pre + Post
         DISPLAY Fib
         Pre = Post
         Post = Fib
      END FOR
   END IF

end procedure
